<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
	version="2.4">
	<context-param>
		<param-name>dbname</param-name>
		<param-value>oracle</param-value>
	</context-param>
	<context-param>
		<param-name>dbusername</param-name>
		<param-value>test</param-value>
	</context-param>

	<listener>
		<listener-class>com.conduent.servlet.listener.ServletContextListenerExample</listener-class>
	</listener>
	<listener>
		<listener-class>com.conduent.servlet.listener.SessionListenerExample</listener-class>
	</listener>

	<servlet>
		<servlet-name>userjsp</servlet-name>
		<jsp-file>/user.jsp</jsp-file>
		<init-param>
			<param-name>drivername</param-name>
			<param-value>oracle driver</param-value>
		</init-param>
	</servlet>

	<servlet-mapping>
		<servlet-name>userjsp</servlet-name>
		<url-pattern>/ujsp</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>second</servlet-name>
		<servlet-class>com.conduent.servlet.ServletPractice</servlet-class>
		<init-param>
			<param-name>username</param-name>
			<param-value>sudhakar</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>second</servlet-name>
		<url-pattern>/second</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>first</servlet-name>
		<servlet-class>com.conduent.servlet.FirstServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>first</servlet-name>
		<url-pattern>/first</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>welcome</servlet-name>
		<servlet-class>com.conduent.servlet.WelcomeServlet</servlet-class>
		<init-param>
			<param-name>username</param-name>
			<param-value>sudhakar</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>welcome</servlet-name>
		<url-pattern>/wc</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.conduent.servlet.login.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>httplogin</servlet-name>
		<servlet-class>com.conduent.servlet.login.LoginHttpServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>httplogin</servlet-name>
		<url-pattern>/hlogin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>httpregister</servlet-name>
		<servlet-class>com.conduent.servlet.login.RegisterHttpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>httpregister</servlet-name>
		<url-pattern>/rlogin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>pwelcome</servlet-name>
		<servlet-class>com.conduent.servlet.login.WelcomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pwelcome</servlet-name>
		<url-pattern>/pwelcome</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>staging</servlet-name>
		<servlet-class>com.conduent.servlet.login.StagingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>staging</servlet-name>
		<url-pattern>/stagingServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>com.conduent.servlet.login.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>download</servlet-name>
		<servlet-class>com.conduent.servlet.login.DownloadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>download</servlet-name>
		<url-pattern>/download</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>opgen</servlet-name>
		<servlet-class>com.conduent.servlet.login.OpgenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>opgen</servlet-name>
		<url-pattern>/opgenServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>downloadopgen</servlet-name>
		<servlet-class>com.conduent.servlet.login.DownloadOpgenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>downloadopgen</servlet-name>
		<url-pattern>/downloadopgen</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>implservlet</servlet-name>
		<servlet-class>com.conduent.servlet.login.ImplementationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>implservlet</servlet-name>
		<url-pattern>/implServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>downloadimpl</servlet-name>
		<servlet-class>com.conduent.servlet.login.DownloadImplServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>downloadimpl</servlet-name>
		<url-pattern>/downloadimpl</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>threadservlet</servlet-name>
		<servlet-class>com.conduent.servlet.login.ThreadSafeDemoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>threadservlet</servlet-name>
		<url-pattern>/threadservlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>displayimage</servlet-name>
		<servlet-class>com.conduent.servlet.login.DisplayImageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>displayimage</servlet-name>
		<url-pattern>/displayimage</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>upload</servlet-name>
		<servlet-class>com.conduent.servlet.login.UploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>upload</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>user</servlet-name>
		<servlet-class>com.conduent.servlet.login.UsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>user</servlet-name>
	<url-pattern>/user</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>group</servlet-name>
	<servlet-class>com.conduent.servlet.login.GroupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>group</servlet-name>
	<url-pattern>/group</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>taglib</servlet-name>
		<jsp-file>/WEB-INF/taglib.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>taglib</servlet-name>
		<url-pattern>/taglib</url-pattern>
		</servlet-mapping>
	

	<filter>
		<filter-name>auth</filter-name>
		<filter-class>com.conduent.filter.AuthenticationFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>auth</filter-name>
		<url-pattern>/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</filter-mapping>

	<error-page>
		<error-code>500</error-code>
		<location>/error.jsp</location>
	</error-page>

	<error-page>
		<exception-type>java.lang.Exception</exception-type>
		<location>/error.jsp</location>
	</error-page>

	<session-config>
		<session-timeout>2</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>header.jsp</welcome-file>
	</welcome-file-list>
</web-app>